  <div class="row container" >
    <div class="container">

          <table class="table">
        <thead >
          <tr>
            <th scope="col">Case #</th>
            <th scope="col">Client First</th>
            <th scope="col">Client Last</th>
            <th scope="col">PC First</th>
            <th scope="col">PC Last</th>
            <th scope="col">BI First</th>
            <th scope="col">BI Last</th>
          </tr>
        </thead>
        <tbody>
            <% @cases.each do |c|%>
                          <tr>
            <th scope="row"><%= link_to "Case #{c.id}", case_path(c) %> </th>
            <td><%=c.client.first_name%></td>
            <td><%=c.client.last_name%></td>
            <td><%=c.pc.first_name%></td>
            <td><%=c.pc.last_name%></td>
            <td><%=c.bi.first_name%></td>
            <td><%=c.bi.last_name%></td>
          </tr>
            <%end%>
        </tbody>
      </table>
     </div>
      </div>